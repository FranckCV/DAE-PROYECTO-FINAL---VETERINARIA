	<queryString>
		<![CDATA[SELECT
    m.id,
    m.nombre AS medicamento,
    m.costo,
    m.stock,
    m.presentacion,
    CASE
        WHEN m.vigencia = true THEN 'Disponible'
        WHEN m.vigencia = false THEN 'No Disponible'
    END AS disponibilidad,
    tp.nomtipo AS tipo_medicamento
FROM
    medicamento m
INNER JOIN
    tipo_medicamento tp
ON
    m.tipo_medicamento_id = tp.id
ORDER BY
    m.id;]]>
	</queryString>

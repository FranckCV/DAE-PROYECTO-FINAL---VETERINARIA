	<queryString>
		<![CDATA[SELECT
     Du."nombres",
     Du.apePaterno,
     Du.apeMaterno,
     Du."doc_identidad",
     Du."telefono",
     Du."correo",
     Du."direccion",
     Du."vigencia",
     Du."telefonoalt" AS Du_telefonoalt,
     Du."sexo" AS Du_sexo,
     Du."id" AS Du_id,
     CU."fecha_adopción" AS CU_fecha_adopción,
     CU."mascotaid" AS CU_mascotaid

	<parameter name="parAnioSub" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
    (DATE_TRUNC('month', c.fecha) + INTERVAL '1 month' - INTERVAL '1 day')::DATE AS fin_mes,
    TO_CHAR(DATE_TRUNC('month', c.fecha), 'TMMonth') AS nombre_mes,
    TO_CHAR(DATE_TRUNC('month', c.fecha), 'TMMon') AS abreviacion_mes,
    'Medicamentos' AS nombre_serie,
    ROUND(SUM(CASE WHEN dm.medicamento_id IS NOT NULL THEN dm.cantidad * m.costo ELSE 0 END), 2) AS valor
FROM
    COMPROBANTE c
LEFT JOIN CITA ci ON c.CITAid = ci.id
LEFT JOIN DETALLE_CITA dc ON ci.id = dc.cita_id
LEFT JOIN DETALLE_SERVICIO ds ON dc.detalle_servicio_serv_id = ds.servicio_id AND dc.detalle_servicio_med_id = ds.medico_id
LEFT JOIN SERVICIO s ON ds.servicio_id = s.id
LEFT JOIN DETALLE_MEDICAMENTO dm ON dm.detalle_cita_id = dc.cita_id AND dm.detalle_servicio_servicio_id = dc.detalle_servicio_serv_id AND dm.detalle_servicio_medico_id = dc.detalle_servicio_med_id
LEFT JOIN MEDICAMENTO m ON dm.medicamento_id = m.id
WHERE
    EXTRACT(YEAR FROM c.fecha) = $P{parAnioSub}
GROUP BY
    DATE_TRUNC('month', c.fecha)
UNION ALL
SELECT
    (DATE_TRUNC('month', c.fecha) + INTERVAL '1 month' - INTERVAL '1 day')::DATE AS fin_mes,
    TO_CHAR(DATE_TRUNC('month', c.fecha), 'TMMonth') AS nombre_mes,
    TO_CHAR(DATE_TRUNC('month', c.fecha), 'TMMon') AS abreviacion_mes,
    'Servicios' AS nombre_serie,
    ROUND(SUM(CASE WHEN dc.detalle_servicio_serv_id IS NOT NULL THEN s.costo ELSE 0 END), 2) AS valor
FROM
    COMPROBANTE c
LEFT JOIN CITA ci ON c.CITAid = ci.id
LEFT JOIN DETALLE_CITA dc ON ci.id = dc.cita_id
LEFT JOIN DETALLE_SERVICIO ds ON dc.detalle_servicio_serv_id = ds.servicio_id AND dc.detalle_servicio_med_id = ds.medico_id
LEFT JOIN SERVICIO s ON ds.servicio_id = s.id
LEFT JOIN DETALLE_MEDICAMENTO dm ON dm.detalle_cita_id = dc.cita_id AND dm.detalle_servicio_servicio_id = dc.detalle_servicio_serv_id AND dm.detalle_servicio_medico_id = dc.detalle_servicio_med_id
LEFT JOIN MEDICAMENTO m ON dm.medicamento_id = m.id
WHERE
    EXTRACT(YEAR FROM c.fecha) = $P{parAnioSub}
GROUP BY
    DATE_TRUNC('month', c.fecha)
ORDER BY
    fin_mes, nombre_serie;]]>
	</queryString>
